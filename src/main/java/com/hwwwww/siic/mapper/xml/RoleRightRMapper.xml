<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hwwwww.siic.mapper.RoleRightRMapper">
    <delete id="del" parameterType="map">
        delete from role_right_r where roleid=#{roleid} and rightid=#{rightid}
    </delete>
    <delete id="delbyroleid" parameterType="int">
        delete from role_right_r where roleid=#{roleid}
    </delete>
</mapper>